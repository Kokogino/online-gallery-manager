{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.8.0",
    "generators": {
      "backend": {
        "generatorName": "typescript-angular",
        "output": "#{cwd}/src/app/gen/ogm-backend",
        "glob": "../backend/contract/ogm-backend.yaml",
        "typeMappings": {
          "set": "Array"
        }
      }
    }
  }
}
